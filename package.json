    "eslint": "8.57.1",
    "npm-run-all": "2.1.0",
    "prisma": "5.21.1",
    "semistandard": "17.0.0",
    "tsup": "8.3.5",
    "tsx": "4.19.2",
    "typescript": "5.6.3",
    "vite-tsconfig-paths": "5.1.0",
    "vitest": "2.1.4"
  },
  "dependencies": {
    "@fastify/jwt": "9.0.1",
    "@prisma/client": "5.21.1",
